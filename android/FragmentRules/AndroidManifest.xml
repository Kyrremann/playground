        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.Default"
        tools:ignore="NewApi" >
        <activity
            android:name="no.minimon.fragmentrules.Loading"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="no.minimon.fragmentrules.FragmentRules" >
        </activity>
        <activity android:name="no.minimon.fragmentrules.FragmentRulesHoneyComb" >
        </activity>
        <activity
            android:name="no.minimon.fragmentrules.SettingsActivity"
            android:label="@string/title_activity_settings" >
        </activity>
        <activity
            android:name="no.minimon.fragmentrules.SettingsActivityHoneyComb"
            android:label="@string/title_activity_settings" >
        </activity>
    </application>

</manifest>